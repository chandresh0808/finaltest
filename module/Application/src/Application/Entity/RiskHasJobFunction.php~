<?php

namespace Application\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * RiskHasJobFunction
 *
 * @ORM\Table(name="risk_has_job_function", indexes={@ORM\Index(name="risk_id", columns={"risk_id", "job_function_id"})})
 * @ORM\Entity
 */
class RiskHasJobFunction
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(name="risk_id", type="integer", nullable=false)
     */
    private $riskId;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="job_function_id", type="datetime", nullable=false)
     */
    private $jobFunctionId;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="created_dt_tm", type="datetime", nullable=true)
     */
    private $createdDtTm;

    /**
     * @var boolean
     *
     * @ORM\Column(name="updated_dt_tm", type="boolean", nullable=true)
     */
    private $updatedDtTm;

    /**
     * @var boolean
     *
     * @ORM\Column(name="delete_flag", type="boolean", nullable=false)
     */
    private $deleteFlag = '0';


}
