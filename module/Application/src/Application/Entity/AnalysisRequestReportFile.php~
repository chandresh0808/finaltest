<?php

namespace Application\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * AnalysisRequestReportFile
 *
 * @ORM\Table(name="analysis_request_report_file", indexes={@ORM\Index(name="analysis_request_id", columns={"analysis_request_id"})})
 * @ORM\Entity
 */
class AnalysisRequestReportFile
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="analysis_request_id", type="string", length=50, nullable=false)
     */
    private $analysisRequestId;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="created_dt_tm", type="datetime", nullable=true)
     */
    private $createdDtTm;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="updated_dt_tm", type="datetime", nullable=true)
     */
    private $updatedDtTm;

    /**
     * @var boolean
     *
     * @ORM\Column(name="delete_flag", type="boolean", nullable=false)
     */
    private $deleteFlag = '0';


}
